gzip              on;
gzip_buffers      16 8k;
gzip_comp_level   1;
gzip_http_version 1.1;
gzip_disable      "MSIE [1-6].(?!.*SV1)";
gzip_min_length   20;
gzip_proxied      any;
gzip_types        text/plain text/css application/json application/x-javascript text/xml application/xml text/javascript;
gzip_vary         on;